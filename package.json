{
  "name": "fmt-str",
  "version": "0.0.0",
  "packageManager": "pnpm@8.4.0",
  "description": "Format string tool.",
  "author": "alexzhang1030",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/fmt-str.git"
  },
  "keywords": [
    "format",
    "string"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "build": "tsup",
    "prepublishOnly": "pnpm run test && pnpm run build",
    "release": "bumpp --commit \"chore: release v%s\" --push --tag && pnpm publish",
    "prepare": "simple-git-hooks",
    "test": "vitest"
  },
  "dependencies": {
    "camelcase": "^7.0.1",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.38.6",
    "@types/deep-freeze-strict": "^1.1.0",
    "@types/pluralize": "^0.0.29",
    "eslint": "^8.40.0",
    "lint-staged": "^13.2.2",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "vitest": "^0.31.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml,yml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-ts"
  }
}
